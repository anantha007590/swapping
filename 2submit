#include <stdio.h>
int main ()
{
    int a,b;
    printf("enter first number=");
    scanf("%d",&a);
    printf("\nenter second number=");
    scanf("%d",&b);
    printf("first number=%d\n",a);
    printf("second number=%d\n",b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("\nafter swapping\n");
printf("first number=%d\n",a) ;
printf("second number=%d\n",b);
return 0;
}
